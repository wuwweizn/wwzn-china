{
  "name": "Lucky",
  "version": "2.18.1",
  "slug": "lucky",
  "codenotary": "djhhui@gmail.com",
  "description": "A Lucky add-on for Home Assistant. Multi-arch Docker image (amd64, arm64, armv7).",
  "image": "ghcr.io/wuwweizn/lucky",
  "arch": ["amd64","aarch64","armv7"],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "ingress": true,
  "ingress_port": 16601,
  "panel_icon": "mdi:server-network",
  "map": [
    "config:rw",
    "data:rw"
  ],
  "ports": {
    "16601/tcp": 16601
  },
  "webui": "http://[HOST]:[PORT:16601]"
}
