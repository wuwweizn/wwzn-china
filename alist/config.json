{
  "name": "Alist",
  "version": "3.45.0",
  "slug": "alist",
  "description": "🗂️多存储文件列表/WebDAV程序 - 支持60+种存储后端的统一文件管理系统",
  "url": "https://github.com/wuwweizn/wwzn-china/tree/main/alist",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "map": ["config:rw", "share:rw", "backup:rw", "media:rw"],
  "options": {
    "web_port": 5244,
    "admin_username": "admin",
    "admin_password": "",
    "log_level": "INFO",
    "enable_aria2": false,
    "enable_ffmpeg": false,
    "enable_webdav": true,
    "custom_config": {}
  },
  "schema": {
    "web_port": "port",
    "admin_username": "str",
    "admin_password": "password",
    "log_level": "list(DEBUG|INFO|WARN|ERROR)",
    "enable_aria2": "bool",
    "enable_ffmpeg": "bool", 
    "enable_webdav": "bool",
    "custom_config": "dict?"
  },
  "ports": {
    "5244/tcp": 5244
  },
  "ports_description": {
    "5244/tcp": "Alist Web管理界面和文件访问端口"
  },
  "host_network": false,
  "privileged": [],
  "full_access": false,
  "homeassistant_api": false,
  "hassio_api": true,
  "docker_api": false,
  "audio": false,
  "video": true,
  "gpio": false,
  "usb": true,
  "uart": false,
  "stdin": false,
  "legacy": false,
  "environment": {
    "PUID": "0",
    "PGID": "0",
    "UMASK": "022",
    "TZ": "Asia/Shanghai"
  },
  "image": "ghcr.io/wuwweizn/alist-hassio",
  "webui": "http://[HOST]:[PORT:5244]",
  "panel_icon": "mdi:folder-multiple-outline",
  "panel_title": "Alist",
  "panel_admin": true,
  "ingress": false,
  "ingress_port": 0,
  "ingress_entry": "/",
  "timeout": 10,
  "tmpfs": "size=100m,uid=0,rw,noatime,nosuid,nodev,noexec,mode=0755",
  "services": []
}